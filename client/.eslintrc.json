{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": ["react", "react-hooks", "prettier"],  // Add "prettier" to the plugins array
    "rules": {
      // Add customize rules here as per your project's needs
      "prettier/prettier": "error"  // Enforce Prettier rules
    }
}
